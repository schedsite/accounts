{
  "name": "sched-accounts-app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod accounts && ng build --prod",
    "pagesbuild": "ng build --prod accounts && ng build --prod --base-href /accounts/",
    "test": "ng test",
    "quiettest": "ng test sched-accounts-app --no-progress && ng test accounts --no-progress",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildlib": "ng build --prod accounts",
    "npmpack": "cd dist/accounts && npm pack",
    "package": "npm run buildlib && npm run npmpack",
    "publish": "npm run package && cd dist/accounts && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/animations": "^6.0.2",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/router": "^6.0.2",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0-beta.1",
    "@angular-devkit/build-ng-packagr": "~0.7.0-beta.1",
    "@angular/cli": "~6.0.3",
    "@angular/compiler-cli": "^6.0.2",
    "@angular/language-service": "^6.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.1.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "ts-node": "~6.0.5",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.10.0",
    "typescript": "~2.7.2"
  }
}
